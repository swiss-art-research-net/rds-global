rds:OntologyExtensions a owl:Ontology ;
    rdfs:label "RDS Ontology Extensions" .

rds:GndPersonShape a sh:NodeShape ;
  sh:targetClass gnd:Person ;
  sh:property
  [
    sh:path gnd:dateOfBirth ;
    Platform:projectionVariableName "dateOfBirth" ;
    dash:propertyRole dash:DescriptionRole ;
  ], [
    sh:path gnd:dateOfDeath ;
    Platform:projectionVariableName "dateOfDeath" ;
    dash:propertyRole dash:DescriptionRole ;
    #sh:datatype xsd:date ;
  ], [
    sh:path gnd:professionOrOccupation ;
    Platform:projectionVariableName "occupation" ;
    dash:propertyRole dash:DescriptionRole ;
  ], [
    sh:path gnd:biographicalOrHistoricalInformation ;
    Platform:projectionVariableName "bio" ;
    dash:propertyRole dash:DescriptionRole ;
  ], [
    sh:path gnd:geographicAreaCode ;
    Platform:projectionVariableName "place" ;
    dash:propertyRole dash:DescriptionRole ;
  ]
  .


  rds:WikidataPersonShape a sh:NodeShape ;
    sh:targetClass wdt:Q1233456 ;
    sh:property
    [
      sh:path ulan:bornAt ;
      Platform:projectionVariableName "dateOfBirth" ;
      dash:propertyRole dash:DescriptionRole ;
    ], [
      sh:path ulan:diedAt ;
      Platform:projectionVariableName "dateOfDeath" ;
      dash:propertyRole dash:DescriptionRole ;
      #sh:datatype xsd:date ;
    ], [
      sh:path gnd:professionOrOccupation ;
      Platform:projectionVariableName "occupation" ;
      dash:propertyRole dash:DescriptionRole ;
    ], [
      sh:path gnd:biographicalOrHistoricalInformation ;
      Platform:projectionVariableName "bio" ;
      dash:propertyRole dash:DescriptionRole ;
    ], [
      sh:path gnd:geographicAreaCode ;
      Platform:projectionVariableName "place" ;
      dash:propertyRole dash:DescriptionRole ;
    ]
    .
